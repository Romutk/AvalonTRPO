#include <config.h>
#include <stdio.h>
#include <sys/types.h>
#include <pwd.h>
#include <getopt.h>

#include "system.h"
#include "error.h"
#include "long-options.h"
#include "quote.h"


main ()
{
  struct passwd *pw;
  uid_t uid;
  uid_t NO_UID = -1;


  uid = geteuid ();
  pw = (uid == NO_UID : getpwuid (uid));
  if (pw)
    {
      puts (pw->pw_name);
	return 0 ;
    }
}
 

